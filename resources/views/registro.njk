<!doctype html>
<html>
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <title>Cine GO | Recorriendo tu Alma</title>
  <!-- Tell the browser to be responsive to screen width -->
  <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
  <!-- Bootstrap 3.3.7 -->
  <link rel="stylesheet" href="bootstrap/css/bootstrap.min.css">
  <!-- Font Awesome -->
  <link rel="stylesheet" href="admin_template/dist/css/AdminLTE.min.css">
  <link rel="stylesheet" href="admin_template/dist/css/skins/_all-skins.min.css">

  <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
<script src="admin_template/plugins/jQuery/jquery-3.1.1.min.js"></script>
<!-- jQuery UI 1.11.4 -->
<script src="https://code.jquery.com/ui/1.11.4/jquery-ui.min.js"></script>
<script src="js/js/js_registro.js"></script>
</head>
<body class="hold-transition login-page">

        <div class="box box-primary"  id="Pnl_Registro" style="display: none;">
            <div class="box-header with-border">
              <h3 class="box-title" id="titulo_formulario">Catalogo Clientes</h3>
            </div>
            <form id="formulario" role="form" method="POST" action="/nvo_cte">
            {{ csrfField }}
              <div class="box-body">
                <div class="form-group">
                  <label for="nombres">Nombres</label>
                  <input type="hidden" class="form-control" id="id_persona" name="id_persona" placeholder="Ingrese su nombre(s)">
                  <input style="text-transform: uppercase;" type="text" class="form-control" id="nombres" name="nombres" placeholder="Ingrese su nombre(s)">
                </div>
                <div class="form-group">
                  <label for="apellidos">Apellidos</label>
                  <input style="text-transform: uppercase;" type="text" class="form-control" id="apellidos" name="apellidos" placeholder="Ingrese sus apellidos">
                </div>
                <div class="form-group">
                  <label for="email">Email</label>
                  <input type="email" class="form-control" id="email" name="email" placeholder="Email">
                </div>
                <div class="form-group">
                  <label for="numero_aut">Numero Tarjeta</label>
                  <input type="text" class="form-control" id="numero_aut" name="numero_aut" placeholder="Numero de Tarjeta" disabled>
                </div>
                <div class="form-group pwd">
                  <label for="password">Password</label>
                  <input type="text" class="form-control" id="password" name="password" placeholder="Ingrese su Password">
                </div>
              </div>
              <div class="box-footer">
                <button type="submit" class="btn btn-primary">Guardar</button>
                <button type="button" id="Btn_Cancela" class="btn btn-primary">Cancelar</button>
              </div>
            </form>
          </div>
    </body>
</html>