<script src="admin_template/plugins/datatables/dataTables.bootstrap.min.js"></script>
<script src="admin_template/plugins/fastclick/fastclick.js"></script>
    
<!-- daterangepicker -->
<script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.11.2/moment.min.js"></script>
<script src="admin_template/plugins/daterangepicker/daterangepicker.js"></script>
<!-- datepicker -->
<script src="admin_template/plugins/datepicker/bootstrap-datepicker.js"></script>
<!-- Bootstrap WYSIHTML5 -->
<script src="admin_template/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.all.min.js"></script>
<!-- Slimscroll -->
<script src="admin_template/plugins/slimScroll/jquery.slimscroll.min.js"></script>
<!-- FastClick -->
<script src="admin_template/plugins/fastclick/fastclick.js"></script>
<!-- AdminLTE App -->
<script src="admin_template/dist/js/adminlte.min.js"></script>
<!-- AdminLTE dashboard demo (This is only for demo purposes) -->
<script src="admin_template/dist/js/pages/dashboard.js"></script>
<script src="admin_template/dist/js/demo.js"></script>
<script type="text/javascript">
$(document).ready(function(){
     $.ajax({
         url:'/DUser',
         type:'post',
         data:{usuario: '{{currentUser.numero_aut}}'},
         datatype:'json'
     }).done(function(response){
        $.each(response,function(a,b){
            $(".NombreUsuario").text(response[a].nombres + " " + response[a].apellidos);
            $(".NombreUs").text(response[a].nombres);
        });// final each
    });//final ajax
    
    
});
</script>

<script src="js/js/js_principal.js"></script>
    
</body>
</html>
