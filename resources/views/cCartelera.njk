        <div class="box box-primary"  id="Pnl_Registro" style="display: none;">
            <div class="box-header with-border">
              <h3 class="box-title" id="titulo_formulario">Catalogo Clientes</h3>
            </div>
            <form id="formulario" role="form" method="POST" action="/nvo_cte">
            {{ csrfField }}
              <div class="box-body">
                <div class="form-group">
                  <label for="nombres">Nombres</label>
                  <input type="hidden" class="form-control" id="id_persona" name="id_persona" placeholder="Ingrese su nombre(s)">
                  <input style="text-transform: uppercase;" type="text" class="form-control" id="nombres" name="nombres" placeholder="Ingrese su nombre(s)">
                </div>
                <div class="form-group">
                  <label for="apellidos">Apellidos</label>
                  <input style="text-transform: uppercase;" type="text" class="form-control" id="apellidos" name="apellidos" placeholder="Ingrese sus apellidos">
                </div>
                <div class="form-group">
                  <label for="email">Email</label>
                  <input type="email" class="form-control" id="email" name="email" placeholder="Email">
                </div>
                <div class="form-group">
                  <label for="numero_aut">Numero Tarjeta</label>
                  <input type="text" class="form-control" id="numero_aut" name="numero_aut" placeholder="Numero de Tarjeta" disabled>
                </div>
                <div class="form-group pwd">
                  <label for="password">Password</label>
                  <input type="text" class="form-control" id="password" name="password" placeholder="Ingrese su Password">
                </div>
              </div>
              <div class="box-footer">
                <button type="submit" class="btn btn-primary">Guardar</button>
                <button type="button" id="Btn_Cancela" class="btn btn-primary">Cancelar</button>
              </div>
            </form>
